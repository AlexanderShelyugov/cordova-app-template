{
  "name": "vite-vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "cordova-prepare": "vite build --emptyOutDir --outDir cordova-app/www",
    "cordova-build": "yarn cordova-prepare && cd ./cordova-app && cordova build android",
    "cordova-run": "yarn cordova-prepare && cd ./cordova-app && cordova emulate android"
  },
  "dependencies": {
    "primevue": "^3.52.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
